{
    "name": "The Simpsons Image Changer Chrome Extension",
    "description": "Change all images on a website to The Simpsons",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "The Simpsons Image Changer"
    }
}